{
  "name": "Member",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "postinstall": "node ./bin/postInstall",
    "android": "react-native run-android --mode=AnitDebug",
    "test:e2e": "detox test -c ios.sim.debug",
    "build:e2e": "detox build -c ios.sim.debug",
    "ci:test:e2e": "detox test -c ios.sim.release -l verbose --cleanup",
    "ci:build:e2e": "detox build -c ios.sim.release",
    "lint": "eslint index.js app storybook test --fix --ext .js,.ts,.tsx && npm run format",
    "patch": "patch-package",
    "storybook": "start-storybook -p 9001 -c ./storybook",
    "splash:generate": "npx react-native generate-bootsplash ../assets/images/logo.png --background=#1B588A   --logo-width=200 --assets-output=../assets --flavor=main",
    "test": "jest",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean": "react-native-clean-project",
    "android:build-release": "cd android && ./gradlew assembleRelease",
    "clean-all": "npx react-native clean-project-auto",
    "android:anit": "SET ENVFILE=.env && react-native run-android --mode=anitDebug",
    "android:db": "ENVFILE=.env.debug react-native run-android --variant=dealerbundleDebug --appId=com.autocarenetwork.dealerbundle",
    "ios:anit": "ENVFILE=.env.debug react-native run-ios --scheme \"WashubMember\"",
    "ios:db": "ENVFILE=.env.debug react-native run-ios --scheme \"Dealer Bundle\""
  },
